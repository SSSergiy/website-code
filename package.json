{
  "name": "website-code-hugo",
  "version": "1.0.0",
  "description": "Hugo-based static website generator",
  "type": "module",
  "scripts": {
    "dev": "hugo server --port 1313",
    "build": "hugo --minify",
    "clean": "hugo --cleanDestinationDir",
    "preview": "hugo server --port 1313 --navigateToChanged",
    "format": "echo 'Use manual formatting for Hugo templates'"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "node-fetch": "^3.3.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.0",
    "postcss-sort-media-queries": "^5.2.0",
    "sass": "^1.85.1"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "prettier-plugin-go-template": "^0.0.14"
  }
}

